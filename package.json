{
  "name": "s3-prometheus-exporter",
  "version": "0.0.8",
  "description": "S3 data exporter for prometheus",
  "author": "Tchoupinax <corentinfiloche@hotmail.fr> (https://corentinfiloche.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tchoupinax/s3-exporter"
  },
  "bugs": {
    "url": "https://github.com/Tchoupinax/s3-exporter/issues"
  },
  "scripts": {
    "watch": "nodemon --exec vite-node src/index.ts",
    "lint": "eslint . --ext=.ts",
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "s3": "docker compose up -d",
    "security": "trivy fs --security-checks vuln,config .",
    "docker:build": "docker buildx build --platform linux/amd64,linux/arm64 -t tchoupinax/s3-prometheus-exporter . --push && docker buildx build --platform linux/amd64,linux/arm64 -t tchoupinax/s3-prometheus-exporter:v$(cat package.json | jq -r '.version') . --push"
  },
  "keywords": [],
  "dependencies": {
    "aws-sdk": "^2.1253.0",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "fastify": "^4.9.2",
    "moment": "^2.29.4",
    "on-finished": "^2.4.1",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "pretty-bytes": "^6.0.0",
    "prom-client": "^14.1.0",
    "vite-node": "^0.25.1",
    "vitest": "^0.25.1"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  }
}
